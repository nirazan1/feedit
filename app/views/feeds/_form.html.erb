<% if user_signed_in? %>
  <%= form_for @feed do |f| %>
   
    <% if @feed.errors.any? %>
      <div id="error_explanation">
        <h2>
          <%= pluralize(@feed.errors.count, "error") %> prohibited
          this feed from being saved:
        </h2>
        <ul>
          <% @feed.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
        </ul>
      </div>
    <% end %> 
    <p>
      
    </p>
    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p> 
    <p>
      <%= f.submit %>
    </p>
  <% end %>
<% else %>
  <p>You are not Signed In, Please
  <l><%= link_to "Sign up", new_user_registration_path %> or <%= link_to "Sign In", new_user_session_path %></li>
  to Post your feed ! </p>
<% end %>