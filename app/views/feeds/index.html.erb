<div class="col-md-6">
	<% if user_signed_in? %>
		<%= render 'form'%>
	<% else %>
		<p>You are not Signed In , please 
		<%= link_to "Sign Up", new_user_registration_path %>
		or Sign In below to Post your Feed !</p>
		<%= render 'devise/sessions/form' %>
		<%= link_to "Forgot your password?", new_password_path(resource_name) %>
	<% end %>
</div>
<div class="col-md-6" id="container_feeds">
	<%= render @feeds.reverse %>
</div>